@model IEnumerable<ResultNavbarLinkDto>

@{
	Layout = "_AdminLayout";
	ViewData["Title"] = "FooterCenter";
}

<h1 class="h3 mb-2 text-gray-800">Footer Center</h1>
<p class="mb-4">Shows Footer Center Table</p>

<!-- DataTales Example -->
<div class="card shadow mb-4">
	@* <div class="card-header py-3">
	<h6 class="m-0 font-weight-bold text-primary">DataTables Example</h6>
	</div> *@
	<div class="card-body">
		<div class="table-responsive">
			<table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
				<thead>
					<tr>
						<th>#</th>
						<th>Text</th>
						<th>Controller</th>
						<th>Action</th>
						<th>Update</th>
						<th>Delete</th>
					</tr>
				</thead>
				<tfoot>
					<tr>
						<th>#</th>
						<th>Text</th>
						<th>Controller</th>
						<th>Action</th>
						<th>Update</th>
						<th>Delete</th>
					</tr>
				</tfoot>
				<tbody>
					@foreach (var item in Model)
					{
						<tr>
							<td>@item.NavbarLinkId</td>
							<td>@item.NavbarLinkText</td>
							<td>@item.NavbarLinkController</td>
							<td>@item.NavbarLinkAction</td>
							<td>
								<a href="/NavbarLink/UpdateNavbarLink/@item.NavbarLinkId" class="btn btn-success">Update</a>
							</td>
							<td>
								<a href="/NavbarLink/DeleteNavbarLink/@item.NavbarLinkId" class="btn btn-danger">Delete</a>
							</td>
						</tr>
					}
				</tbody>

			</table>
			<a asp-controller="NavbarLink" asp-action="CreateNavbarLink" class="btn btn-warning">Create New NavbarLink</a>
		</div>
	</div>
</div>
