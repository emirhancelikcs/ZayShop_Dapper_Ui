@model ResultInfosDto

@{
	Layout = "_AdminLayout";
	ViewData["Title"] = "Infos";
}

<h1>@ViewData["Title"]</h1>

<!-- Page Heading -->
<h1 class="h3 mb-2 text-gray-800">Infos</h1>
<p class="mb-4">Shows Infos Table</p>

<!-- DataTales Example -->
<div class="card shadow mb-4">
	@* <div class="card-header py-3">
	<h6 class="m-0 font-weight-bold text-primary">DataTables Example</h6>
	</div> *@
	<div class="card-body">
		<div class="table-responsive">
			<table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
				<thead>
					<tr>
						<th>#</th>
						<th>Email</th>
						<th>Phone</th>
						<th>Facebook</th>
						<th>Instagram</th>
						<th>Twitter</th>
						<th>Linkedin</th>
						<th>Update</th>
					</tr>
				</thead>
				<tfoot>
					<tr>
						<th>#</th>
						<th>Email</th>
						<th>Phone</th>
						<th>Facebook</th>
						<th>Instagram</th>
						<th>Twitter</th>
						<th>Linkedin</th>
						<th>Update</th>
					</tr>
				</tfoot>
				<tbody>
					<tr>
						<td>@Model.InfosId</td>
						<td>@Model.InfosEmail</td>
						<td>@Model.InfosPhone</td>
						<td>@Model.InfosFacebookAddress</td>
						<td>@Model.InfosInstagramAddress</td>
						<td>@Model.InfosTwitterAddress</td>
						<td>@Model.InfosLinkedinAddress</td>
						<td>
							<a href="/Infos/UpdateInfos/@Model.InfosId" class="btn btn-success">Update</a>
						</td>
					</tr>
				</tbody>
			</table>
		</div>
	</div>
</div>
