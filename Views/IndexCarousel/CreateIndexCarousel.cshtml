@model CreateIndexCarouselDto

@{
	Layout = "_AdminLayout";
	ViewData["Title"] = "Create IndexCarousel";
}

<h1>@ViewData["Title"]</h1>

<form method="post" enctype="multipart/form-data">
	<label>IndexCarousel Header</label>
	<input type="text" asp-for="Header" class="form-control">
	<br>
	<label>IndexCarousel Title</label>
	<input type="text" asp-for="Title" class="form-control">
	<br>
	<label>IndexCarousel Description</label>
	<input type="text" asp-for="Description" class="form-control">
	<br>
	<label>IndexCarousel ImageName</label>
	<input type="file" name="formFile" class="form-control">
	<br>
	<button class="btn btn-primary">Save the IndexCarousel</button>
</form>
