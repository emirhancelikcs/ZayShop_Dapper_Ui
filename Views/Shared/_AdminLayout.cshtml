@{
	Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@await Component.InvokeAsync("_AdminHeaderViewComponentPartial")

<body id="page-top">

	<!-- Page Wrapper -->
	<div id="wrapper">
		<partial name="_NotificationPartial" />

		@await Component.InvokeAsync("_AdminSidebarViewComponentPartial")

		<!-- Content Wrapper -->
		<div id="content-wrapper" class="d-flex flex-column">

			<!-- Main Content -->
			<div id="content">
				<!-- Topbar -->
				@await Component.InvokeAsync("_AdminNavbarViewComponentPartial")
				<!-- End of Topbar -->
				<!-- Begin Page Content -->
				<div class="container-fluid">
					@RenderBody()
				</div>
				<!-- /.container-fluid -->

			</div>
			<!-- End of Main Content -->
			<!-- Footer -->
			@await Component.InvokeAsync("_AdminFooterViewComponentPartial")
			<!-- End of Footer -->

		</div>
		<!-- End of Content Wrapper -->

	</div>
	<!-- End of Page Wrapper -->
	<!-- Scroll to Top Button-->
	<a class="scroll-to-top rounded" href="#page-top">
		<i class="fas fa-angle-up"></i>
	</a>

	<!-- Logout Modal-->
	@await Component.InvokeAsync("_AdminLogoutViewComponentPartial")

	<!-- Bootstrap core JavaScript-->
	<script src="~/vendor/jquery/jquery.min.js"></script>
	<script src="~/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

	<!-- Core plugin JavaScript-->
	<script src="~/vendor/jquery-easing/jquery.easing.min.js"></script>

	<!-- Custom scripts for all pages-->
	<script src="~/js/sb-admin-2.min.js"></script>
	@await RenderSectionAsync("Scripts", required: false)
</body>

</html>

